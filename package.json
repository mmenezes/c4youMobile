{
  "name": "GetMyPills",
  "version": "1.0.0",
  "description": "This is the client web application using React, Node.js, Express and Webpack. It is configured with webpack-dev-server, eslint and babel. Tests are configured using Jest and Enzyme.",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "build": "webpack --mode production --json > stats.json",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint --quiet src"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>src/__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/setup/",
      "<rootDir>/src/__tests__/__mocks__/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "author": "G3NiU7",
  "license": "",
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "connected-react-router": "^4.3.0",
    "cookies-js": "^1.2.3",
    "express": "^4.16.3",
    "globalize": "^1.4.2",
    "immutability-helper": "^2.8.1",
    "immutable": "^3.8.2",
    "jquery": "^3.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-html-parser": "^2.0.2",
    "react-https-redirect": "^1.1.0",
    "react-linkify": "^1.0.0-alpha",
    "react-loader-spinner": "^2.3.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.12",
    "react-test-renderer": "^15.5.0",
    "react-widgets": "^4.4.11",
    "react-widgets-globalize": "^5.0.20",
    "react-widgets-moment": "^4.0.27",
    "reactstrap": "^6.5.0",
    "recharts": "^1.5.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-plugin-recharts": "^1.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "nodemon": "^1.17.3",
    "react-addons-test-utils": "^15.6.2",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.14"
  }
}
